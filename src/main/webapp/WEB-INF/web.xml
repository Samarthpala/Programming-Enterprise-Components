<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>TransferServlet</servlet-name>
  	<display-name>TransferServlet</display-name>
  	<description></description>
  	<servlet-class>com.bank.servlet.TransferServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransactionApiServlet</servlet-name>
  	<display-name>TransactionApiServlet</display-name>
  	<description></description>
  	<servlet-class>com.bank.servlet.TransactionApiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TransferServlet</servlet-name>
  	<url-pattern>/TransferServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransactionApiServlet</servlet-name>
  	<url-pattern>/TransactionApiServlet</url-pattern>
  </servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
